[coyote]
# These queue to runner mappings (<queue_name>:<runner_name>) propulate the
#   queues list to run through during runtime.
queue-mappings =

[amqp]
host = localhost
port = 5672
user = guest
password = guest

###
# logging configuration
###

[loggers]
keys = root, coyote

[handlers]
keys = console

[formatters]
keys = generic

[logger_root]
level = INFO
handlers = console

[logger_coyote]
level = DEBUG
handlers =
qualname = coyote

[handler_console]
class = StreamHandler
args = (sys.stderr,)
level = NOTSET
formatter = generic

[formatter_generic]
format = %(asctime)s %(levelname)-5.5s [%(name)s] %(message)s
